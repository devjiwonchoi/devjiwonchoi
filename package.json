{
  "name": "devjiwonchoi",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "turbo": "canary",
    "typescript": "^5.6.3",
    "vercel": "^37.7.1"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{css,json,md,mdx,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "packageManager": "pnpm@9.6.0"
}
