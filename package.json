{
  "name": "jiwonchoi.dev",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "tb": "pnpm turbo build",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "canary",
    "react": "19.0.0-rc.0",
    "react-dom": "19.0.0-rc.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "next",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/edge-config": "^1.2.1",
    "@vercel/speed-insights": "^1.0.12",
    "geist": "^1.3.1",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "resend": "^3.5.0",
    "tailwindcss": "next",
    "turbo": "canary",
    "typescript": "^5.5.4",
    "vercel": "canary"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
      "prettier --write"
    ],
