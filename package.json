{
  "name": "devjiwonchoi",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tb": "turbo build",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/speed-insights": "^1.1.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "canary",
    "next": "canary",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resend": "^4.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "turbo": "^2.3.3",
    "vercel": "^39.2.6"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
      "next lint --max-warnings=0 --fix --file",
      "prettier --write"
    ],
    "*.{css,json,md,mdx,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {}
    }
  },
  "packageManager": "pnpm@9.6.0"
}
